msgctxt "Description|"
msgid "Time Series"
msgstr "时间序列"

msgctxt "Description|"
msgid "This module offers time series analyses."
msgstr "该模块提供时间序列分析."

msgctxt "Description|"
msgid "Descriptives"
msgstr "描述统计"

msgctxt "Description|"
msgid "Stationarity"
msgstr "平稳性"

msgctxt "Description|"
msgid "ARIMA"
msgstr "ARIMA 模型"

