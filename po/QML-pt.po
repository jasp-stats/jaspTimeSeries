msgstr "e.g., resíduos"

msgctxt "ARIMATimeSeries|"
msgid "Forecasting"
msgstr "Previsão"

msgctxt "ARIMATimeSeries|"
msgid "Number of forecasts"
msgstr "Número de previsões"

msgctxt "ARIMATimeSeries|"
msgid "Determines the number forecasts to make."
msgstr "Determina o número de previsões a serem feitas."

msgctxt "ARIMATimeSeries|"
msgid "Save forecasts as"
msgstr "Salvar previsões como"

msgctxt "ARIMATimeSeries|"
msgid "e.g. forecasts.csv"
msgstr "e.g. previsoes.csv"

msgctxt "ARIMATimeSeries|"
msgid "Saves the forecasts in a seperate .csv file."
msgstr "Salva as previsões em um arquivo .csv separado."

msgctxt "ARIMATimeSeries|"
msgid "Plots the forecasts (and observed values) (y-axis) over time (x-axis)"
msgstr ""
"Traça as previsões (e valores observados) (eixo y) ao longo do tempo (eixo x)"

msgctxt "ARIMATimeSeries|"
