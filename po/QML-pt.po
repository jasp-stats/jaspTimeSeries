msgstr "Logaritmo"

msgctxt "StationarityTimeSeries|"
msgid "Takes the log of the dependent variable."
msgstr "Considera o logaritmo da variável dependente."

msgctxt "StationarityTimeSeries|"
msgid "Base 10"
msgstr "Base 10"

msgctxt "StationarityTimeSeries|"
msgid "Base e"
msgstr "Base e"

msgctxt "StationarityTimeSeries|"
msgid "Root"
msgstr "Raiz"

msgctxt "StationarityTimeSeries|"
msgid "Takes the root of the dependent variable."
msgstr "Extrai a raiz da variável dependente."

msgctxt "StationarityTimeSeries|"
